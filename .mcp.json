{
  "mcpServers": {
    "github": {
      "type": "http",
      "url": "https://mcp.github.com"
    },
    "svelte-docs": {
      "type": "http",
      "url": "https://svelte-llm.stanislav.garden/mcp/mcp"
    },
    "astro": {
      "type": "sse",
      "url": "http://localhost:4321/__mcp/sse"
    },
    "shadcn-svelte": {
      "type": "stdio",
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@jpisnice/shadcn-ui-mcp-server@latest"
      ],
      "env": {
        "FRAMEWORK": "svelte"
      }
    }
  }
}
